{
  "name": "Apti",
  "description": "Apti Ä°mza",
  "version": "1.0",
  "manifest_version": 2,
  "main": "index.js",
  "permissions": ["storage", "activeTab", "scripting","tabs"],
  "action": {
    "default_icon": "/icons/48.png"
  },
  "icons": {
    "16": "/icons/16.png",
    "32": "/icons/32.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "http://localhost:3000/*",
        "https://apti-signature.vercel.app/*"
      ],
      "js": ["index.js"],
      "css": ["main.css"]
    }
  ]
}
